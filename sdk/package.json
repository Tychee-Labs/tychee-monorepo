{
    "name": "@tychee/sdk",
    "version": "0.1.2",
    "description": "Card tokenization SDK for Stellar blockchain with regulatory compliance and Web3 self-custody",
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "require": "./dist/index.js",
            "import": "./dist/index.mjs",
            "types": "./dist/index.d.ts"
        }
    },
    "files": [
        "dist",
        "README.md",
        "LICENSE"
    ],
    "scripts": {
        "build": "tsc",
        "build:watch": "tsc --watch",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "lint": "eslint src --ext .ts",
        "prepublishOnly": "npm run build",
        "clean": "rm -rf dist",
        "prepare": "npm run build"
    },
    "keywords": [
        "card-tokenization",
        "stellar",
        "soroban",
        "blockchain",
        "web3",
        "payments",
        "encryption",
        "aes-256-gcm",
        "self-custody",
        "fintech",
        "regulatory-compliance",
        "account-abstraction"
    ],
    "author": {
        "name": "Tychee Labs",
        "email": "ops@tychee.store",
        "url": "https://tychee.store"
    },
    "license": "BSL-1.1",
    "homepage": "https://github.com/tychee-labs/tychee#readme",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/tychee-labs/tychee.git",
        "directory": "sdk"
    },
    "bugs": {
        "url": "https://github.com/tychee-labs/tychee/issues",
        "email": "ops@tychee.store"
    },
    "engines": {
        "node": ">=18.0.0"
    },
    "sideEffects": false,
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.org/"
    },
    "dependencies": {
        "@noble/curves": "^1.3.0",
        "@noble/hashes": "^1.3.3",
        "@stellar/stellar-sdk": ">=12.0.0"
    },
    "devDependencies": {
        "@types/jest": "^29.5.11",
        "@types/libsodium-wrappers": "^0.7.14",
        "@types/node": "^20.10.0",
        "jest": "^29.7.0",
        "typescript": "^5.3.0"
    },
    "peerDependencies": {
        "@stellar/stellar-sdk": ">=12.0.0"
    },
    "peerDependenciesMeta": {
        "@stellar/stellar-sdk": {
            "optional": false
        }
    }
}